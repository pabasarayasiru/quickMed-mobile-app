{
  "expo": {
    "name": "quickMed",
    "slug": "quickMed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.quickmed.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/quickmed.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ],
      "usesCleartextTraffic": true,
        "plugins": [
    [
      "expo-location",
      {
        "locationWhenInUsePermission": "Allow QuickMed to access your location."
      }
    ]
  ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c9e100e8-1b72-4280-8ac6-ea9282fc45b7"
      }
    }
  }
}
